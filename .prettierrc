{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "useTabs": false,
  "importOrder": [
    "^next",
    "^react",
    "^@src/(.*)$",
    "^@images/(.*)$",
    "^@icons/(.*)$",
    "^@mui/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
